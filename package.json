{
  "name": "vuex-mock-store",
  "version": "0.0.0",
  "description": "Simple and straightforward mock for Vuex v3.x Store",
  "main": "src/index.js",
  "author": {
    "name": "Eduardo San Martin Morote",
    "email": "posva13@gmail.com"
  },
  "scripts": {
    "lint": "eslint --color --ext=js,html src test",
    "unit": "jest",
    "dev": "npm run unit -- --watchAll",
    "test": "npm run lint && npm run unit"
  },
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "vue",
    "test",
    "utils",
    "test-utils",
    "store",
    "vuex",
    "mock",
    "vuex-mock",
    "vuex-store-mock",
    "jest",
    "vuex-mock-store"
  ],
  "license": "MIT",
  "devDependencies": {
    "codecov": "^3.1.0",
    "eslint": "^5.6.0",
    "eslint-config-posva": "^2.0.1",
    "jest": "^23.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vuex-mock-store.git"
  },
  "bugs": {
    "url": "https://github.com/posva/vuex-mock-store/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/*.js",
      "<rootDir>/test/.*.js",
      "<rootDir>/test/*/*.js"
    ]
  },
  "homepage": "https://github.com/posva/vuex-mock-store#readme"
}
